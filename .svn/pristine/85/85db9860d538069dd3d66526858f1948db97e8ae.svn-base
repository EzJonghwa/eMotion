<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
	  <!-- Oracle 테이블의 컬럼이 mem_id 이고, DTO의 필드변수가 memId 일때 매칭이 가능하게 하는 옵션 -->
	  <setting name="mapUnderscoreToCamelCase" value="true"/> 
	  <!-- 널 허용 -->
	  <setting name="jdbcTypeForNull" value="NULL" />
	  <!-- 쿼리 로그 확인가능 -->
	  <setting name="logImpl" value="STDOUT_LOGGING"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.eMotion.team.member.vo.MemberVO" alias="MemberVO" />
		<typeAlias type="com.eMotion.team.recommend.vo.RecommendVO" alias="RecommendVO" />
		<typeAlias type="com.eMotion.team.recommend.vo.FilterVO" alias="FilterVO" />
		<typeAlias type="com.eMotion.team.recommend.vo.SearchVO" alias="SearchVO" />
		<typeAlias type="com.eMotion.team.recommend.vo.LocalVO" alias="LocalVO" />
		<typeAlias type="com.eMotion.team.car.vo.CarVO" alias="CarVO" />
		<typeAlias type="com.eMotion.team.board.vo.BoardVO" alias="BoardVO" />
		<typeAlias type="com.eMotion.team.board.vo.BoardSearchVO" alias="BoardSearchVO" />
		<typeAlias type="com.eMotion.team.board.vo.CommentVO" alias="CommentVO" />
		<typeAlias type="com.eMotion.team.routine.vo.RoutineVO" alias="RoutineVO" />
		<typeAlias type="com.eMotion.team.routine.vo.ElecCarInfoVO" alias="ElecCarVO" />
		<typeAlias type="com.eMotion.team.routine.vo.GasCarInfoVO" alias="GasCarVO" />
		<typeAlias type="com.eMotion.team.message.vo.MessageVO" alias="MessageVO" />
		<typeAlias type="com.eMotion.team.routine.vo.maxDataVO" alias="maxDataVO" />
	</typeAliases>
	</typeAliases>
</configuration>